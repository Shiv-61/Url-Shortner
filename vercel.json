{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Views/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.js"
    },
    {
      "src": "/url",
      "dest": "index.js"
    },
    {
      "src": "/:shortCode",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "Views/$1"
    }
  ]
}
